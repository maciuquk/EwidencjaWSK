@*@model IEnumerable<EwidencjaWSK.Models.Audit>*@
@model IEnumerable<EwidencjaWSK.ViewModel.AuditViewModel>;

@{
    ViewData["Title"] = "Changes";
}

<h1>Zmiany</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Table)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ChangedBy)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BeforeChanges)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AfterChanges)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Table)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ChangedBy)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BeforeChanges)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AfterChanges)
            </td>
        </tr>
}
    </tbody>
</table>
